{
  "expo": {
    "name": "TransfaApp",
    "slug": "transfa-app",
    "scheme": "transfa",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "main": "index.js",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.transfaapp",
      "associatedDomains": ["applinks:trytransfa.com", "applinks:www.trytransfa.com"]
    },
    "android": {
      "package": "com.transfaapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            {
              "scheme": "https",
              "host": "trytransfa.com"
            },
            {
              "scheme": "https",
              "host": "www.trytransfa.com"
            },
            {
              "scheme": "transfa"
            }
          ]
        }
      ]
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Transfa to save request QR codes to your photo library.",
          "savePhotosPermission": "Allow Transfa to save request QR codes to your photo library."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c7c4c27f-4418-47fd-b61c-c481a1f37af1"
      }
    }
  }
}
