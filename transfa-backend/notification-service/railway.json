{
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "transfa-backend/notification-service/Dockerfile"
  },
  "deploy": {
    "startCommand": "./notification-service",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10,
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300
  }
}